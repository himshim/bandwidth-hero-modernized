{
  "manifest_version": 3,
  "name": "Bandwidth Hero",
  "version": "0.0.7",
  "description": "Save data by compressing images",
  "permissions": [
    "declarativeNetRequest",
    "storage",
    "offscreen"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/popup.html"
  },
  "options_page": "options/options.html",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}